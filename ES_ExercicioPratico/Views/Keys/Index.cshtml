@model IEnumerable<ES_ExercicioPratico.Models.Key>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Criar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Number1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Number5)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Star1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Star2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsRandomKey)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Number1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number5)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Star1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Star2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsRandomKey)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Detalhes</a>
                <a asp-action="Delete" asp-route-id="@item.Id">Apagar</a>
            </td>
        </tr>
}
    </tbody>
</table>
